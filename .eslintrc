{
  "plugins": [
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": ["error", {"trailingComma": "es5", "singleQuote": true}]
  },
  "extends": [
    "prettier"
  ]
}
